<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.newpath.activity.RangeWheesLife"
    android:background="@color/colorBackgroundForm"
    android:fillViewport="true">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.user.newpath.activity.RegisterEmail"
        android:background="@color/colorBackgroundForm">

        <ImageView
            android:id="@+id/imgLogoSup"
            android:layout_width="121dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/logo_sup_o_03"
            android:layout_marginLeft="28dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginStart="28dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.021"
            android:layout_marginEnd="8dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="176dp"
            android:layout_height="134dp"
            app:srcCompat="@drawable/details_wheels_life"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="5dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.943" />

        <TextView
            android:id="@+id/textView"
            android:textColor="@color/colorFontPrincipal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nome"
            android:layout_marginTop="22dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/edtNome"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="@+id/edtNome"
            app:layout_constraintHorizontal_bias="0.0" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email"
            android:textColor="@color/colorFontPrincipal"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/edtNascimento"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="@+id/edtNascimento"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/edtNascimento" />

        <TextView
            android:id="@+id/Senha"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Senha"
            android:textColor="@color/colorFontPrincipal"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/edtEmail"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="@+id/edtEmail"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/edtEmail" />

        <EditText
            android:id="@+id/edtNome"
            android:layout_width="310dp"
            android:textColor="@color/colorFontPrincipal"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.494"
            android:layout_marginTop="21dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" />

        <EditText
            android:id="@+id/edtEmail"
            android:layout_width="310dp"
            android:layout_height="45dp"
            android:ems="10"
            android:textColor="@color/colorFontPrincipal"
            android:inputType="textPersonName"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView4"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="@+id/textView4"
            app:layout_constraintHorizontal_bias="0.028"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <EditText
            android:id="@+id/edtSenha"
            android:layout_width="310dp"
            android:layout_height="45dp"
            android:ems="10"
            android:textColor="@color/colorFontPrincipal"
            android:inputType="textPassword"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/Senha"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="@+id/Senha"
            app:layout_constraintHorizontal_bias="0.028"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/Senha" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cadastro"
            android:textColor="@color/colorFontPrincipal"
            android:textSize="30sp"
            app:layout_constraintTop_toTopOf="@+id/guideline10"
            android:layout_marginTop="73dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <Button
            android:id="@+id/btnGravar"
            android:layout_width="134dp"
            android:layout_height="48dp"
            android:text="Cadastrar"
            android:textColor="@color/colorButton"
            android:background="@drawable/button_bg_radius"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/edtSenha"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="@+id/edtSenha"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="29dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/edtSenha" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline10"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.13050076" />

        <EditText
            android:id="@+id/edtNascimento"
            android:layout_width="312dp"
            android:layout_height="45dp"
            android:ems="10"
            android:inputType="date"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/dtNascimento"
            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="@+id/dtNascimento"
            app:layout_constraintHorizontal_bias="0.031"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/dtNascimento" />
        <TextView
            android:id="@+id/dtNascimento"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Aniversario"
            android:textColor="@color/colorFontPrincipal"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/edtNome"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="@+id/edtNome"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/edtNome" />

    </android.support.constraint.ConstraintLayout>
</ScrollView>

