<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.newpath.activity.RangeWheesLife"
    android:background="@color/colorBackgroundForm"
    android:fillViewport="true">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.user.newpath.activity.RangeWheesLife"
        android:background="@color/colorBackgroundForm">

        <ImageView
            android:id="@+id/imgLogoSup"
            android:layout_width="121dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/logo_sup_o_03"
            android:layout_marginLeft="28dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginStart="28dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.026"
            android:layout_marginEnd="8dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/rangeWheels"
            android:layout_marginTop="19dp"
            app:layout_constraintTop_toTopOf="@+id/guideline"
            android:textSize="17sp"
            android:textColor="@color/colorFontPrincipal"
            app:layout_constraintRight_toLeftOf="@+id/guideline7"
            android:layout_marginRight="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/guideline6"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="17dp"
            android:text="@string/obsWheels"
            android:textColor="@color/colorFontSecondary"
            android:textSize="15sp"
            app:layout_constraintTop_toBottomOf="@+id/textView10"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/guideline6"
            app:layout_constraintRight_toLeftOf="@+id/guideline7"
            android:layout_marginRight="8dp" />


        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.1"
            tools:layout_editor_absoluteY="91dp"
            tools:layout_editor_absoluteX="0dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline6"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.09"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="37dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline7"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.91"
            tools:layout_editor_absoluteY="25dp"
            tools:layout_editor_absoluteX="374dp" />



                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="1"
                    android:text="@string/personalGrowth"
                    android:textColor="@color/colorFontPrincipal"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/textView11"
                    android:layout_marginRight="8dp"
                    app:layout_constraintRight_toRightOf="@+id/textView11"
                    android:layout_marginTop="22dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView11"
                    app:layout_constraintHorizontal_bias="0.45"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp" />

                <SeekBar
                    android:id="@+id/seek_cres_pessoal"
                    android:layout_width="277dp"
                    android:layout_height="33dp"
                    android:max="10"
                    android:progress="0"
                    android:progressTint="@color/colorFontPrincipal"
                    android:thumb="@drawable/custom_thumb"
                    android:layout_marginTop="7dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView12"
                    android:layout_marginRight="8dp"
                    app:layout_constraintRight_toRightOf="@+id/textView12"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/textView12"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp" />

                <TextView
                    android:id="@+id/txt_crescimento"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/colorFontSecondary"
                    app:layout_constraintTop_toTopOf="@+id/seek_cres_pessoal"
                    app:layout_constraintBottom_toBottomOf="@+id/seek_cres_pessoal"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintVertical_bias="1.0"
                    app:layout_constraintLeft_toRightOf="@+id/seek_cres_pessoal"
                    android:layout_marginLeft="22dp"
                    android:layout_marginStart="22dp" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:ellipsize="end"
            android:lines="1"
            android:text="@string/career"
            android:textColor="@color/colorFontPrincipal"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintLeft_toLeftOf="@+id/textView12"
            app:layout_constraintRight_toRightOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/seek_cres_pessoal"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" />

                <SeekBar
                    android:id="@+id/seek_carreira"
                    android:layout_width="277dp"
                    android:layout_height="33dp"
                    android:max="10"
                    android:progress="0"
                    android:progressTint="@color/colorFontPrincipal"
                    android:thumb="@drawable/custom_thumb"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/seek_cres_pessoal"
                    android:layout_marginRight="8dp"
                    app:layout_constraintRight_toRightOf="@+id/seek_cres_pessoal"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView14"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp" />

                <TextView
                    android:id="@+id/txt_carreira"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/colorFontSecondary"
                    app:layout_constraintTop_toTopOf="@+id/seek_carreira"
                    android:layout_marginTop="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/seek_carreira"
                    app:layout_constraintLeft_toRightOf="@+id/seek_carreira"
