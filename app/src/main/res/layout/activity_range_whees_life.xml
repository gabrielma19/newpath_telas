                    android:layout_marginStart="22dp" />

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="1"
                    android:text="@string/social"
                    android:textColor="@color/colorFontPrincipal"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/seek_saude"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/textView15"
                    android:layout_marginRight="8dp"
                    app:layout_constraintRight_toRightOf="@+id/textView15"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp" />

                <SeekBar
                    android:id="@+id/seek_social"
                    android:layout_width="277dp"
                    android:layout_height="33dp"
                    android:max="10"
                    android:progress="0"
                    android:progressTint="@color/colorFontPrincipal"
                    android:thumb="@drawable/custom_thumb"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/seek_saude"
                    app:layout_constraintRight_toRightOf="@+id/seek_saude"
                    android:layout_marginTop="7dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView16"
                    android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="1.0" />

                <TextView
                    android:id="@+id/txt_amigos"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/colorFontSecondary"
                    app:layout_constraintBottom_toBottomOf="@+id/seek_social"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintVertical_bias="0.0"
                    android:layout_marginStart="18dp"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="@+id/seek_social"
                    app:layout_constraintLeft_toRightOf="@+id/seek_social"
                    android:layout_marginLeft="10dp" />

                <TextView
                    android:id="@+id/textView15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="1"
                    android:text="@string/health"
                    android:textColor="@color/colorFontPrincipal"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/seek_financa"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/textView13"
                    app:layout_constraintRight_toRightOf="@+id/textView13"
                    android:layout_marginStart="8dp" />

                <SeekBar
                    android:id="@+id/seek_saude"
                    android:layout_width="277dp"
                    android:layout_height="33dp"
                    android:max="10"
                    android:progress="0"
                    android:progressTint="@color/colorFontPrincipal"
                    android:thumb="@drawable/custom_thumb"
                    app:layout_constraintLeft_toLeftOf="@+id/seek_financa"
                    app:layout_constraintRight_toLeftOf="@+id/txt_financas"
                    android:layout_marginRight="8dp"
                    app:layout_constraintHorizontal_bias="0.0"
