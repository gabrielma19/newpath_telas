                    android:textColor="@color/colorFontSecondary"
                    app:layout_constraintTop_toTopOf="@+id/seek_saude"
                    android:layout_marginTop="8dp"
                    app:layout_constraintBottom_toBottomOf="@+id/seek_saude"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintLeft_toRightOf="@+id/seek_saude"
                    android:layout_marginLeft="22dp"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="1"
                    android:text="@string/business"
                    android:textColor="@color/colorFontPrincipal"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/textView14"
                    app:layout_constraintRight_toRightOf="@+id/textView14"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/seek_carreira"
                    app:layout_constraintHorizontal_bias="0.533"
                    android:layout_marginStart="8dp" />

                <SeekBar
                    android:id="@+id/seek_financa"
                    android:layout_width="277dp"
                    android:layout_height="33dp"
                    android:max="10"
                    android:progress="0"
                    android:progressTint="@color/colorFontPrincipal"
                    android:thumb="@drawable/custom_thumb"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/seek_carreira"
                    android:layout_marginRight="8dp"
                    app:layout_constraintRight_toRightOf="@+id/seek_carreira"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView13"
                    android:layout_marginStart="8dp"
                    android:layout_marginEnd="8dp" />

                <TextView
                    android:id="@+id/txt_financas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="0"
                    android:textColor="@color/colorFontSecondary"
                    app:layout_constraintTop_toTopOf="@+id/seek_financa"
                    app:layout_constraintBottom_toBottomOf="@+id/seek_financa"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintVertical_bias="1.0"
                    app:layout_constraintLeft_toRightOf="@+id/seek_financa"
                    android:layout_marginLeft="22dp"
                    android:layout_marginStart="22dp" />

                <Button
                    android:id="@+id/btn_next"
                    android:layout_width="206dp"
                    android:layout_height="48dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:background="@drawable/button_bg_radius"
                    android:text="Continuar"
                    android:textColor="@color/colorButton"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/seek_social"
                    android:layout_marginLeft="8dp"
                    app:layout_constraintLeft_toLeftOf="@+id/seek_social"
                    android:layout_marginRight="8dp"
                    app:layout_constraintRight_toRightOf="@+id/seek_social"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintHorizontal_bias="0.636"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintVertical_bias="0.636" />

    </android.support.constraint.ConstraintLayout>
</ScrollView>
